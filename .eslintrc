{
  "extends": ["airbnb", "prettier"],
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "jest": true
  },
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["error"],
    "no-underscore-dangle": [2, { "allowAfterThis": false, "allow": ["_id"] }],
    "react/no-array-index-key": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/no-multi-comp": 0,
    "import/no-unresolved": 0,
    "max-len": [2, 100, 2],
    "import/extensions": 0
  },
  "globals": {
    "React": true,
    "classNames": true,
    "PropTypes": true
  }
}
